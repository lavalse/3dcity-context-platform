# PostgreSQL / 3DCityDB
POSTGRES_DB=citydb
POSTGRES_USER=citydb
POSTGRES_PASSWORD=citydb
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# 3DCityDB spatial reference (JGD2011 geographic 2D)
# EPSG:6668 = JGD2011 geographic 2D (used by PLATEAU CityGML)
SRID=6668
HEIGHT_EPSG=0

# LLM (Anthropic Claude)
ANTHROPIC_API_KEY=   # leave empty for placeholder mode; set real sk-ant-... key for Claude API mode

# Backend
BACKEND_PORT=8000

# Frontend (served by nginx)
FRONTEND_PORT=3000
